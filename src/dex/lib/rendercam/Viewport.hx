package dex.lib.rendercam;

typedef Viewport = {
	/** The viewport X offset (black bar width) for fixed aspect ratio cameras. **/
	var x:Float;

	/** The viewport Y offset (black bar height) for fixed aspect ratio cameras. **/
	var y:Float;

	/** The viewport width. **/
	var width:Float;

	/** The viewport height. **/
	var height:Float;
}
